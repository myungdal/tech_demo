{
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "sd_xl_base_1.0.safetensors"
    }
  },
  "2": {
    "class_type": "IPAdapterUnifiedLoader",
    "inputs": {
      "model": ["1", 0],
      "preset": "VIT-G (medium strength)"
    }
  },
  "3": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{CHARACTER_IMAGE}}"
    }
  },
  "4": {
    "class_type": "IPAdapter",
    "inputs": {
      "model": ["2", 0],
      "ipadapter": ["2", 1],
      "image": ["3", 0],
      "weight": 0.9,
      "start_at": 0.0,
      "end_at": 1.0,
      "weight_type": "style transfer"
    }
  },
  "5": {
    "class_type": "ControlNetLoader",
    "inputs": {
      "control_net_name": "controlnet-openpose-sdxl.safetensors"
    }
  },
  "6": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{POSE_IMAGE}}"
    }
  },
  "7": {
    "class_type": "OpenposePreprocessor",
    "inputs": {
      "image": ["6", 0],
      "detect_hand": "disable",
      "detect_body": "enable",
      "detect_face": "disable",
      "resolution": 512
    }
  },
  "8": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "clip": ["1", 1],
      "text": "{{POSITIVE_PROMPT}}"
    }
  },
  "9": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "clip": ["1", 1],
      "text": "{{NEGATIVE_PROMPT}}"
    }
  },
  "10": {
    "class_type": "ControlNetApplyAdvanced",
    "inputs": {
      "positive": ["8", 0],
      "negative": ["9", 0],
      "control_net": ["5", 0],
      "image": ["7", 0],
      "strength": 0.8,
      "start_percent": 0.0,
      "end_percent": 1.0
    }
  },
  "11": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 512,
      "height": 512,
      "batch_size": 1
    }
  },
  "12": {
    "class_type": "KSampler",
    "inputs": {
      "model": ["4", 0],
      "positive": ["10", 0],
      "negative": ["10", 1],
      "latent_image": ["11", 0],
      "seed": "{{SEED}}",
      "steps": 25,
      "cfg": 7.0,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0
    }
  },
  "13": {
    "class_type": "VAEDecode",
    "inputs": {
      "vae": ["1", 2],
      "samples": ["12", 0]
    }
  },
  "14": {
    "class_type": "SaveImage",
    "inputs": {
      "images": ["13", 0],
      "filename_prefix": "{{OUTPUT_PREFIX}}"
    }
  }
}
